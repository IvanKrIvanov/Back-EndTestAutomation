					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 200\", function () {",
							"    pm.response.to.have.status(200);",
							"});",
							"",
							"pm.test(\"Response is a non-empty array\", function () {",
							"    pm.expect(pm.response.json()).to.be.an('array').that.is.not.empty;",
							"});",
							"",
							"pm.test(\"Response contains story with matching title\", function () {",
							"    var responseBody = pm.response.json();",
							"    var storyExists = responseBody.some(function(story) {",
							"        return story.title === pm.collectionVariables.get(\"randomTitle\");",
							"    });",
							"    pm.expect(storyExists).to.be.true;",
							"});",
